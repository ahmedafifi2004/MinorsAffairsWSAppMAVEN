<?xml version="1.0" encoding="windows-1256" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
 
  <persistence-unit name="WSMinorsAffairs">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/minorsaffairsDS</jta-data-source>
    <class>wsminorsaffairs.entities.WvResponseCodeLookup</class>
    <class>wsminorsaffairs.entities.VwGafmaDetail</class>
    <properties>
      <property name="eclipselink.target-server" value="WebLogic_10"/>
      <property name="javax.persistence.jtaDataSource" value="jdbc/minorsaffairsDS"/>
      <property name="eclipselink.cache.size.default" value="0"/>
      <property name="eclipselink.query-results-cashe" value="false"/>
      <property name="eclipselink.cache.shared.default" value="false"/>
    </properties>
  </persistence-unit>
</persistence>
